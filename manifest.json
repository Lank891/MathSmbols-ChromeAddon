{
	"manifest_version": 2,
	"name": "Math Symbols",
	"version": "1.0",
	
	"description": "Allows you to copy math symbols and greek alphabet.",
	"icons":
	{
		"16": "icon16.png",
		"48": "icon48.png",
        "128": "icon128.png"
	},
	
	
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_title": "Math Symbols",
		"default_popup": "popup.html"
	},
	
	"author": "Andrzej Litkowski",
	
	
	
	"permissions":
	[
		"clipboardWrite"
	]
}